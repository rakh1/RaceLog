{
  "name": "racelog",
  "version": "1.4.0",
  "description": "Race Car Setup Tracker",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "pkg . --output dist/RaceLog.exe",
    "test": "jest --runInBand --forceExit",
    "test:coverage": "jest --coverage --runInBand --forceExit"
  },
  "pkg": {
    "assets": [
      "public/**/*",
      "default-data/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "session-file-store": "^1.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "pkg": "^5.8.1",
    "supertest": "^6.3.4"
  }
}
